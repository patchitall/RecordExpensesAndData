<div class="container">
<%= simple_form_for @trip do |f| %>
<%= f.error_notification %>
 
  <div class="row">
    <%= f.input :destination, autofocus: true %>
  </div>

  <div class="row">
    <%= f.input :start_date, :as => :date_picker %>
  </div>
  <div class="row">
    <%= f.input :end_date, :as => :date_picker %><br>
 </div>
  <div class="row actions btn">
    <%= f.button :submit %>

  </div>
<% end %>
</div>


