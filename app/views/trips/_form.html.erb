<div class="container">
<%= simple_form_for @trip do |f| %>
<%= f.error_notification %>
  

  <div class="row">
    <%= f.input :start_date, as: :date, html5: true %>
  </div>
  <div class="row">
    <%= f.input :end_date, as: :date, html5: true %><br>
 </div>
  <div class="row">
    <%= f.button :submit %>
  </div>
<% end %>
</div>


